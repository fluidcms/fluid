<% if (hasGroups) { %>
    <nav>
        <ul>
            <% if (hasVariables) { %>
            <li><a>Variables</a></li>
            <% } %>
            <% $.each(groups, function(key, group) { %>
            <li><a data-group="<%= group.name %>"><%= group.name %></a></li>
            <% }) %>
        </ul>
        <a data-action="close">Close</a>
    </nav>
<% } %>

<div data-name="variable-collection" class="variable-collection"></div>